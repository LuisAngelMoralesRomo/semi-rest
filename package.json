{
  "name": "semi-rest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-copy": "copy \"package.electron.json\" \"dist/semi-rest/package.json\" && copy \"electron.js\" \"dist/semi-rest/electron.js\"",
    "build-electron": "ng build --prod --build-optimizer --output-hashing none --base-href ./ && npm run electron-copy",
    "electron-win": " build --win --ia32",
    "electron-win-p": " build --win --ia32 -p always",
    "electron-lnx": "build --linux",
    "electron-lnx-p": "build --linux -p always"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@mdi/font": "^3.6.95",
    "angular-pipes": "^9.0.2",
    "angular2-draggable": "^2.3.1",
    "bootstrap": "^4.3.1",
    "ngx-currency": "^2.0.0",
    "primeng": "^8.1.1",
    "rxjs": "~6.4.0",
    "simple-keyboard": "^2.23.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "electron-builder": "^20.43.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
